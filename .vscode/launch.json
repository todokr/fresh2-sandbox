{
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Program",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "~/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--unstable-temporal",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }
    ]
}
